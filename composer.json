{
    "name": "jennosgroup/laravel-tables",
    "description": "A laravel package that allows you to seemlessly generate html tables entirely using PHP classes.",
    "license": "MIT",
    "authors": [
        {
            "name": "Jenry Ollivierre",
            "email": "jenry@jenryollivierre.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "JennosGroup\\Laratables\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JennosGroup\\Laratables\\Providers\\AppServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
